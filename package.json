{
  "name": "nexon-finance-subgraph",
  "author": "Nexon Finance",
  "version": "0.1.0",
  "scripts": {
    "prepare": "mustache ./config/${NETWORK:-zkSync2-testnet}.json ${BLOCKCHAIN:-zksync}.subgraph.template.yaml > subgraph.yaml && npm run subgraph:codegen",
    "subgraph:codegen": "graph codegen --output-dir ./generated",
    "subgraph:build": "graph build",
    "subgraph:deploy": "graph deploy $SLUG --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --access-token $ACCESS_TOKEN",
    "deploy": "BLOCKCHAIN=zksync NETWORK=zkSync2-testnet env-cmd npm run prepare && SLUG=nexon-finance/nexon-finance npm run subgraph:deploy"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.19.0",
    "@graphprotocol/graph-ts": "^0.19.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "env-cmd": "^10.1.0",
    "mustache": "^4.2.0"
  }
}
